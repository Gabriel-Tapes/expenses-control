{
  "extends": [
    "../../.eslintrc.json",
    "standard",
    "plugin:@typescript-eslint/recommended"
  ],
  "overrides": [],
  "env": {
    "es2021": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-useless-constructor": 0,
    "@typescript-eslint/no-non-null-assertion": "off",
    "array-callback-return": "off"
  }
}
